<div class="container">
    <h1>The team</h1>

    <div class="members">
        @foreach (var member in team)
        {
            <div class="member">
                <img alt="The profile photo of @member.name" src="https://media.licdn.com/dms/image/@member.image">
                <div class="info">
                    <h2>@member.name</h2>
                    <ul class="social">
                        @if (!string.IsNullOrWhiteSpace(member.linkedIn))
                        {
                            <li><a href="https://www.linkedin.com/in/@member.linkedIn" target="_blank">LinkedIn</a></li>
                        }
                        @if (!string.IsNullOrWhiteSpace(member.twitter))
                        {
                            <li><a href="https://twitter.com/@member.twitter" target="_blank">@@@member.twitter</a></li>
                        }
                    </ul>
                    <ul class="responsibilities">
                        @if (member.responsibilities.Any())
                        {
                            <li>Responsibilities:</li>
                        }
                        @foreach (var responsibility in member.responsibilities.OrderBy(x => x))
                        {
                            <li>@responsibility</li>
                        }
                    </ul>
                </div>
            </div>
        }
    </div>
</div>